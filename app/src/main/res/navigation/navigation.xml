<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            xmlns:tools="http://schemas.android.com/tools" android:id="@+id/navigation"
            app:startDestination="@id/allStoriesFragment">

    <fragment android:id="@+id/allStoriesFragment" android:name="techiebits.net.kidstory.mainscreen.AllStoriesFragment"
              android:label="fragment_all_stories" tools:layout="@layout/fragment_all_stories"><action android:id="@+id/action_allStoriesFragment_to_story1Activity" app:destination="@id/story1Activity"/></fragment>
    <fragment android:id="@+id/story1Activity" android:name="techiebits.net.kidstory.story1.Story1Activity"
              android:label="activity_story1" tools:layout="@layout/activity_story1"><action android:id="@+id/action_story1Activity_to_story1Content" app:destination="@id/story1Content"/><action android:id="@+id/action_story1Activity_to_story1Quiz" app:destination="@id/story1Quiz"/>
        <action android:id="@+id/action_story1Activity_pop" app:popUpTo="@id/story1Activity" app:popUpToInclusive="true"/>
    </fragment>
    <fragment android:id="@+id/story1Content" android:name="techiebits.net.kidstory.story1.Story1Content"
              android:label="activity_story1_content" tools:layout="@layout/activity_story1_content">
        <action android:id="@+id/action_story1Content_pop" app:popUpTo="@id/story1Content" app:popUpToInclusive="true"/>
    </fragment>
    <fragment android:id="@+id/story1Quiz" android:name="techiebits.net.kidstory.story1.Story1Quiz"
              android:label="activity_story1_quiz" tools:layout="@layout/activity_story1_quiz">
        <action android:id="@+id/action_story1Quiz_pop" app:popUpTo="@id/story1Quiz" app:popUpToInclusive="true"/>
    </fragment>
    <fragment android:id="@+id/aboutActivity" android:name="techiebits.net.kidstory.aboutscreen.AboutActivity"
              android:label="activity_about" tools:layout="@layout/activity_about"/>
</navigation>